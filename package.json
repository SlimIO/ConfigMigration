{
  "name": "@slimio/config-migration",
  "version": "0.1.0",
  "description": "SlimIO Configuration Migration",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && nyc --reporter=lcov ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "codecov",
    "report": "nyc report --reporter=html"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/ConfigMigration.git"
  },
  "keywords": [
    "SlimIO",
    "config",
    "migration",
    "json"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/ConfigMigration/issues"
  },
  "homepage": "https://github.com/SlimIO/ConfigMigration#readme",
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^4.1.0",
    "@slimio/psp": "^0.12.0",
    "ava": "^3.8.2",
    "codecov": "^3.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "husky": "^4.2.5",
    "jsdoc": "^3.6.4",
    "nyc": "^15.0.1",
    "pkg-ok": "^2.3.1"
  },
  "dependencies": {
    "@slimio/is": "^1.5.1",
    "fast-json-patch": "^3.0.0-1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  }
}
